//find the duplicates elements :
#include <iostream>
using namespace std;


struct Array 
{
    int *A;
    int size;
    int length;
};

void Duplicate_Elements(struct Array arr)
{
    int last_duplicate = 0;
    
    for(int i=0 ;i< arr.length ;i++)
    {
        if(arr.A[i] == arr.A[i+1] && arr.A[i] != last_duplicate)
        {
            cout<<arr.A[i] <<" ";
            last_duplicate = arr.A[i] ;
        }
    }
}



int main()
{
    struct Array arr1;
    arr1.size = 10;
    arr1.length = 9;
    
    arr1.A = new int[arr1.length];
    
    cout<<"enter the elements "<<endl;
    for(int i=0;i< arr1.length; i++)
    {
        cin >>arr1.A[i];
    }
    cout<<endl;
    Duplicate_Elements(arr1);
    
    return 0;
}
