//find the duplicates elements : using hash in unordered array 
#include <iostream>
using namespace std;


struct Array 
{
    int *A;
    int size;
    int length;
};




void Max_Min(struct Array arr)
{
    int max = arr.A[0];
    int min=arr.A[0];
    
    for(int i=0;i<arr.length;i++)
    {
        if(arr.A[i] < min) min = arr.A[i];
        else max = arr.A[i];
    }
    cout<<"max element = " <<max <<endl;
    cout<<"min element = "<< min <<endl;
}

int main()
{
    struct Array arr1;
    arr1.size = 10;
    arr1.length = 9;
    
    arr1.A = new int[arr1.length];
    
    cout<<"enter the elements "<<endl;
    for(int i=0;i< arr1.length; i++)
    {
        cin >>arr1.A[i];
    }
    cout<<endl;
    Max_Min(arr1);
    
    return 0;
}
