#include <iostream>
using namespace std;
int main()
{
    
   char s[] ="WElCoME%";
   int i;
   for(i=0; s[i] != '\0' ; i++)
   {
      if(s[i] >= 'A' && s[i] <= 'Z')
      {
          s[i] += 32 ;
      }
      else if(s[i] >='a' && s[i] <= 'z')
      {
          s[i] -=32;
      }
      
   }
   cout << s << endl;
     
    return 0;
}
